{
    "editor.minimap.enabled": false,
    "vim.sneak": true,
    "vim.easymotion": true,
    "editor.renderWhitespace": "boundary",
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "vim.hlsearch": true,
    "git.autofetch": true,
    "git.autoStash": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["g", "n"],
            "commands": [
                "workbench.action.files.newUntitledFile"
            ]
        },
        {
            "before": ["g", "l"],
            "commands": [
                "workbench.action.nextEditorInGroup"
            ]
        },
        {
            "before": ["g", "L"],
            "commands": [
                "workbench.action.moveEditorRightInGroup"
            ]
        },
        {
            "before": ["g", "h"],
            "commands": [
                "workbench.action.previousEditorInGroup"
            ]
        },
        {
            "before": ["g", "H"],
            "commands": [
                "workbench.action.moveEditorLeftInGroup"
            ]
        },
        {
            "before": ["<C-g>"],
            "after": ["<Esc>"],
        },
        {
            "before": ["<Leader>", "h"],
            "commands": [":nohl"]
        },
        // {
        //     "before": [":"],
        //     "commands": ["workbench.action.showCommands"]
        // },
        // {
        //     "before": ["/"],
        //     "commands": ["actions.find"]
        // },
    ],
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": ["<C-g>"],
            "after": ["<Esc>"],
        },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": ["<C-g>"],
            "after": ["<Esc>"]
        },
        {
            "before": ["!"],
            "commands": ["editWithShell.runCommand"]
        },
        // {
        //     "before": [":"],
        //     "commands": ["workbench.action.showCommands"]
        // },
    ],
    "vim.handleKeys": {
        "<C-n>": false,
        "<C-e>": false,
        "<C-a>": false,
    },
    "workbench.activityBar.visible": false,
    "vim.leader": "<Space>",
    "vim.autoSwitchInputMethod.defaultIM": "com.google.inputmethod.Japanese.Roman",
    "vim.autoSwitchInputMethod.obtainIMCmd": "/usr/local/bin/im-select",
    "vim.autoSwitchInputMethod.switchIMCmd": "/usr/local/bin/im-select {im}",
    "vim.autoSwitchInputMethod.enable": true,
    "workbench.startupEditor": "newUntitledFile",
    "window.zoomLevel": 0,
    "editor.accessibilitySupport": "off",
    "editor.fontFamily": "'Fira Code', Monaco, Menlo, 'Courier New', monospace",
    "workbench.editor.showTabs": true,
    "workbench.editor.enablePreview": false,
    "zenMode.hideTabs": false,
    "files.trimTrailingWhitespace": true,
    "vim.useSystemClipboard": true,
    "editor.parameterHints.enabled": false,
    "editor.fontLigatures": true,
    "editor.renderControlCharacters": true,
    "workbench.colorTheme": "Ayu Adapta",
    "vim.easymotionMarkerHeight": 20,
    "workbench.statusBar.visible": true,
    "zenMode.hideStatusBar": false,
    "workbench.statusBar.feedback.visible": false,
    "workbench.colorCustomizations": {
        "statusBar.foreground": "#A6B9CC",
        "editorLineNumber.foreground": "#4E5863",
    },
    "editor.lineNumbers": "relative",
    "breadcrumbs.enabled": true,
    "workbench.quickOpen.preserveInput": true,
    "window.newWindowDimensions": "maximized",
    "openInGitHub.useCommitSHAInURL": true,
    "openInGitHub.requireSelectionForLines": true,
    "search.smartCase": true,
    "search.collapseResults": "alwaysExpand",
    "search.showLineNumbers": true,
    "vim.visualstar": true,
    "editor.defaultFormatter": "GitHub.vscode-pull-request-github",
    "files.simpleDialog.enable": true,
    "files.trimFinalNewlines": true,
    "breadcrumbs.symbolPath": "off"
}